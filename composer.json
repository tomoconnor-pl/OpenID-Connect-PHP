{
    "name": "tomoconnor-pl/openid-connect-php",
    "description": "Bare-bones OpenID Connect client",
    "keywords": ["oidc", "openid", "jwt", "oauth2"],
    "license": "Apache-2.0",
    "authors": [
        {
        "name": "Tom O'Connor",
        "email": "tom.oconnor@plentific.com"
        },
        
        {
        "name": "Jakub Onderka",
        "email": "jakub.onderka@gmail.com"
    }],
    "require": {
        "php": ">=7.0",
        "phpseclib/phpseclib" : ">=3.0.7",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "roave/security-advisories": "dev-master"
    },
    "archive" : {
        "exclude" : [
            ".*",
            "tests"
        ]
    },
    "suggest": {
        "ext-apcu": "For caching remote server responses"
    },
    "autoload" : {
         "classmap": [ "src/"]
    }
}
